{
  "name": "@johh/three-effectcomposer",
  "version": "0.0.1",
  "description": "three.js's EffectComposer ported to ES6",
  "main": "dist/index.js",
  "scripts": {
    "test": "webpack test/utils/env.js -o test/_tmp/EffectComposer.js && mocha && npm run clean:test",
    "build": "babel src --out-dir dist",
    "dev": "babel src --out-dir dist --watch --source-maps",
    "clean:test": "rm -r test/_tmp",
    "clean:build": "rm -r dist"
  },
  "keywords": [
    "three",
    "three.js",
    "effectcomposer",
    "glsl",
    "shader",
    "postprocessing"
  ],
  "author": "JohH <code@johh.net>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "mocha": "^5.1.1",
    "puppeteer": "^1.3.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "three": "^0.92.0"
  }
}
